{
  "name": "cli",
  "version": "1.0.0",
  "description": "This is a custom CLI",
  "author": "",
  "license": "ISC",
  "type": "module",
  "main": "./src/cli.ts",
  "bin": {
    "norush": "./bin/index.js"
  },
  "scripts": {
    "norush": "node dist/cli.js",
    "norush:dev": "esno src/cli.ts",
    "update-version": "esno src/core/version.ts ",
    "build:config": "pnpm rimraf dist && rollup --c ./scripts/build.config.ts --configPlugin typescript"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "rollup",
    "cli"
  ],
  "dependencies": {
    "cac": "^6.7.14",
    "figlet": "^1.6.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/prompts": "^2.4.4",
    "kolorist": "^1.8.0",
    "prompts": "^2.4.2"
  }
}
