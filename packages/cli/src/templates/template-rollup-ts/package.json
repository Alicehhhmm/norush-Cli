{
  "name": "template-rollup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup.config.js ",
    "build:clear": "rimraf dist",
    "build-bundle": "rollup --config rollup.config.js --configPlugin typescript",
    "build:dev": "rollup src/main.js -f cjs -d dist",
    "build:m2": "rollup src/main.js src/main2.js -f es -d dist",
    "build:bundle": "rollup src/main.js -o bundle.js -f cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "rollup": "^3.25.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
